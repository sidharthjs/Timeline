<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.imwazir.tl" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".newsfeed"
            android:label="@string/title_activity_newsfeed" />
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile" />
        <activity
            android:name=".profile_edit"
            android:label="@string/title_activity_profile_edit" />
        <activity
            android:name=".signup"
            android:label="@string/title_activity_signup" />
        <activity
            android:name=".profile_setup"
            android:label="@string/title_activity_profile_setup" />
        <activity
            android:name=".subscriber"
            android:label="@string/title_activity_subscriber" >
        </activity>
    </application>

</manifest>
